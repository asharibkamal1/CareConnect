    function ABC(obj, tr) {
        debugger
        //var tr = $(this).closest('tr');
        //var srNo = srNo;

        resolveDate = $(tr).parent().prev().find('input[name="reDate"]').val();
        obj.resolveDate = resolveDate;
        obj.oldPhoneNo;
        obj.newPhoneNo
        //if (obj.oldPhoneNo == obj.newPhoneNo) {
        //    swal({
        //        title: "You Can Not Update.",
        //        text:" Old Phone Number and New Phone Number are Same",
        //        icon: "info",
        //    });
        //    return false;
        //}
        //else {
        if (resolveDate == "") {
            swal("Please Select Resolve Date.", {
                icon: "info",
            });
            return false;
        }
        swal({
            title: "Are you sure to resolve?",
            icon: "info",
            buttons: true,
            dangerMode: true,
        })
            .then((willDelete) => {
                if (willDelete) {
                    $(tr).parent().find('button[name="resolve"]').prop('disabled', true);
                    $.ajax({
                        url: "/Admin/ResolvedPreComplaint/",
                        type: "POST",
                        data: JSON.parse(JSON.stringify({ preComplaint: obj })),
                        success: function (res) {
                            if (res == "success") {
                                swal("Complaint successfully Resolved.", {
                                    icon: "success",
                                }).then((value) => {
                                    window.location.href = "/Admin/PreComplaint";
                                    //window.location.reload();
                                });
                            }
                            else if (res == "UserUnAuthorized") {
                                location.url = "Account/Admin/"
                            }
                        },
                        error: function (err) { }
                    });
                } else {
                    window.location.href = "/Admin/PreComplaint";
                    //window.location.reload();
                }
            });

        //}
    }
</script>