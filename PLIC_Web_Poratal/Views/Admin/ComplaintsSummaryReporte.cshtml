@model PLIC_Web_Poratal.Models.ViewModel

@{
    Layout = "_UserLayout";
    ViewData["Title"] = "Complaints Summary Reporte";
}



<!-- Main content -->
<div class="invoice p-3 mb-3 " id="summaryReporte">
    <!-- title row -->
    <div class="col-6">

    </div>
    <div class="row">

        <div class="col-sm-12 center-block text-center">
            <h5><b>Date: </b></h5>
        </div>


    </div>
    <!-- info row -->
    <div class="row ">

        <div class="col-sm-12 center-block text-center">
            <h4><b>PLIC CMS Dash-Board</b></h4>
        </div>


    </div>
    <div class="row ">

        <div class="col-sm-12 center-block text-center">
            <h6><b><label id="CircleWise"> @Model.circle.CirclelName </label> Circle</b></h6>
        </div>


    </div>

    <div class="row ">
        <div class="col-sm-2"></div>
        <div class="col-sm-10">
            <h5><b>Region : <label id="RegionWise"> @Model.region.regionName </label> </b></h5>
        </div>


    </div>

    <div class="row ">
        <div class="col-sm-2"></div>
        <div class="col-sm-10">
            <h5><b>Totle UnResolved Complaints:  </b></h5>
        </div>


    </div>
    <div class="row ">
        <div class="col-sm-2"></div>
        <div class="col-sm-10 ">
            <h5><b>Resolved Within Last 30 Days:  </b></h5>
        </div>


    </div>
    <!-- /.row -->
    <!-- Table row -->
    <div class="row">
        <div class="col-2"></div>
        <div class="col-8 table-responsive">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>Complaints Category</th>
                        <th>
                            Pending for more than 60 days
                            (Unresolved)
                        </th>
                        <th>
                            Pending for more than 30 days
                            (Unresolved)
                        </th>
                        <th>
                            Pending for more than 15 days
                            (Unresolved
                        </th>
                        <th>Received in last 15 days</th>
                    </tr>
                </thead>
                <tbody id="tablebody">
                </tbody>

            </table>
        </div>
        <!-- /.col -->
    </div>

    <div class="row ">
        <div class="col-sm-2"></div>
        <div class="col-sm-10 ">
            <h6><b>Generated By :  </b></h6>
        </div>


    </div>
    <!-- /.row -->
    <!-- this row will not appear when printing -->
    @*<div class="row no-print">
            <div class="col-12">
                <a href="invoice-print.html" rel="noopener" target="_blank" class="btn btn-default"><i class="fas fa-print"></i> Print</a>

            </div>
        </div>*@
</div>
<!-- /.invoice -->
