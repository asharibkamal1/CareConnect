@model CareConnect.Models.TrackingGenerateViewModel
            <!-- Render the dropdown list with TicketCatType data -->

<label for="TicketTypeDropdown">Ticket Description:</label>
<select class="form-control" id="SubcategoryDropdown" name="ticketsubcatagory" onchange="handleDropdownChange(this)">
    @foreach (System.Data.DataRow catRow in Model.TicketIssueTypeDescription.Tables[0].Rows)
    {
        <option value="@catRow["IssueTypeId"]">@catRow["IssueTypeDescription"]</option>
    }
</select>
<span class="text-danger" id="categoryDropdownError" style="display: none;">Please select Sub Category.</span>
